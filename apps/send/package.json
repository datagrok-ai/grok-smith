{
  "name": "@datagrok/send",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./client/app-definition": "./client/src/app-definition.ts",
    "./server/app-definition": "./server/app-definition.ts"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --config client/vite.config.ts",
    "dev:server": "tsx watch server/index.ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx tools/seed.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@datagrok/app-core": "*",
    "@datagrok/app-kit": "*",
    "@datagrok/core-schema": "*",
    "@datagrok/server-kit": "*",
    "@hono/node-server": "^1.11",
    "@hono/zod-validator": "^0.2",
    "drizzle-orm": "1.0.0-beta.15-859cf75",
    "hono": "^4.4",
    "jszip": "^3.10",
    "postgres": "^3.4",
    "react": "^18.3",
    "react-dom": "^18.3",
    "react-router-dom": "^6.23",
    "xport-js": "^0.4.0",
    "zod": "^3.23"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@types/react": "^18.3",
    "@types/react-dom": "^18.3",
    "@vitejs/plugin-react": "^4.3",
    "autoprefixer": "^10.4",
    "concurrently": "^8.2",
    "drizzle-kit": "1.0.0-beta.15-859cf75",
    "postcss": "^8.4",
    "tailwindcss": "^3.4",
    "tsx": "^4.15",
    "vite": "^5.3",
    "vitest": "^1.6"
  }
}
